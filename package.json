{
	"name": "tldraw-in-obsidian",
	"version": "1.26.2",
	"description": "Integrates Tldraw into Obsidian, allowing users to draw and edit content on a virtual whiteboard.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dist": "node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"postinstall": "patch-package",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"tldraw",
		"drawing",
		"whiteboard",
		"notes"
	],
	"author": "Sam Alhaqab",
	"contributors": [
		"Jonathan Mendez <dev@jon-dez.com> (https://github.com/sponsors/jon-dez)",
		"Sam Alhaqab"
	],
	"devDependencies": {
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "7.12.0",
		"@typescript-eslint/parser": "7.12.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.23.1",
		"jsdom": "^27.4.0",
		"obsidian": "^1.10.0",
		"patch-package": "^8.0.0",
		"tslib": "2.6.3",
		"typescript": "^5.5.4",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"monkey-around": "^2.3.0",
		"pdfjs-dist": "^4.10.38",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tldraw": "^4.2.1",
		"zustand": "^4.3.9"
	}
}
